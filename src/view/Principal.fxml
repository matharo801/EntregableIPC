<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.PrincipalController">
    <stylesheets>
        <URL value="@/resources/estilos.css" />
    </stylesheets>
   <children>
      <VBox alignment="CENTER" layoutX="109.0" layoutY="55.0" prefHeight="260.0" prefWidth="349.0" spacing="10.0">
         <children>
            <Label text="Menu Principal">
               <font>
                  <Font name="Arial Nova" size="18.0" />
               </font>
            </Label>
            <Button fx:id="btnIniciarSesion" mnemonicParsing="false" onAction="#iniciarSesion" text="Iniciar Sesion" />
            <Button fx:id="btnRealizarProblema" mnemonicParsing="false" onAction="#iniciarSesion" text="Realizar Problema" />
            <Button fx:id="btnModificarPerfil" mnemonicParsing="false" onAction="#modificarPerfil" text="Modificar Perfil" />
            <Button fx:id="btnMostrarResultados" mnemonicParsing="false" onAction="#mostrarResultados" text="Mostrar Resultados" />
            <Button fx:id="btnCerrarSesion" mnemonicParsing="false" onAction="#cerrarSesion" text="Cerrar sesion" />
         </children>
      </VBox>
   </children>
</AnchorPane>
